Xft.autohint: 0

Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! urxvt
URxvt*saveLines:10000
URxvt*scrollstyle: plain
URxvt*scrollBar_right: true
URxvt*geometry: 84Ã—30
URxvt*font: xft:DejaVu\ Sans Mono\ for\ Powerline-8
!! clickable links
URxvt.perl-ext-common: default,vtwheel,url-select
URxvt.urlLauncher: /usr/bin/firefox
URxvt.urlButton: 3
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.underlineURLs: true
URxvt.letterSpace: -1
